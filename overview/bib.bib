}

@article{Mladenovic2007,
  author={Snezana Mladenovic and  Mirjana Cangalovic},
  title={Heuristic approach to train rescheduling},
  doi={10.2298/yjor0701009m},
  url={http://dx.doi.org/10.2298/yjor0701009m},
