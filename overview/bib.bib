@article{PrataAN23,
title = {Applications of constraint programming in production scheduling problems: A descriptive bibliometric analysis},
journal = {Results in Control and Optimization},
volume = {14},
pages = {100350},
year = {2024},
issn = {2666-7207},
doi = {10.1016/j.rico.2023.100350},
url = {https://www.sciencedirect.com/science/article/pii/S2666720723001522},
author = {Bruno A. Prata and Levi R. Abreu and Marcelo S. Nagano},
keywords = {Production scheduling, Artificial intelligence, Constraint programming, Methodi ordinatio, Bibliometrix},
abstract = {Production scheduling is a class of widely studied combinatorial optimization problems. Given the complexity of the most addressed production environments several solution procedures have been proposed. In recent years, operational researcher practitioners have been paying attention to solving production scheduling problems using constraint programming, and growing interest in this research domain has been evidenced. The aim of this study is to report a descriptive bibliometric analysis of applications of constraint programming in production scheduling problems. The scope of the study is limited to reviewing 170 scientific papers published between 1992 and 2023 from the Scopus and Web of Science databases. In our proposed research questions, we could address the main topics studied, the most studied performance measures, and the profile of the analyzed documents. Furthermore, we could identify the main gaps and present suggestions for future research.}
}

