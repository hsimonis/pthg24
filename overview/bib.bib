@article{Lozano2014,
  author={Roberto CastaÃ±eda Lozano and  Mats Carlsson and  Gabriel Hjort Blindell and  Christian Schulte},
  title={Combinatorial spill code optimization and ultimate coalescing},
  doi={10.1145/2666357.2597815},
  url={http://dx.doi.org/10.1145/2666357.2597815},
