@article{AggounB93,
title = {Extending {CHIP} in order to solve complex scheduling and placement problems},
journal = {Mathematical and Computer Modelling},
volume = {17},
number = {7},
pages = {57-73},
year = {1993},
issn = {0895-7177},
doi = {10.1016/0895-7177(93)90068-A},
url = {https://www.sciencedirect.com/science/article/pii/089571779390068A},
author = {Abderrahmane Aggoun and Nicolas Beldiceanu},
abstract = {In this paper, we show how the introduction of a new primitive constraint over finite domains in the constraint logic programming system CHIP allows us to find very good solutions for a large class of very difficult scheduling and placement problems. Examples on the cumulative scheduling problem, the 10 jobs Ã— 10 machines problem, the perfect square problem, the strip packing problem and the incomparable rectangles packing problem are given, showing the versatility, the efficiency and the broad range of application of this new constraint. We point out that no other existing approach can address simultaneously all the problems discussed in this paper.}
}

