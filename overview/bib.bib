@incollection{Hooker19,
  author={John N. Hooker},
  editor={JesÃºs M. VelÃ¡squez-BermÃºdez and Marzieh Khakifirooz and Mahdi Fathi},
  title={{Logic-Based Benders Decomposition for Large-Scale Optimization}},
  booktitle={{Large Scale Optimization in Supply Chains and Smart Manufacturing}},
  publisher={Springer},
  year=2019,
  month={July},
  volume={},
  number={},
  series={Springer Optimization and Its Applications},
  edition={},
  chapter={},
  pages={1-26},
  doi={10.1007/978-3-030-22788-3_1},
  keywords={},
  abstract={ Logic-based Benders decomposition (LBBD) is a substantial generalization of classical Benders decomposition that, in principle, allows the subproblem to be any optimization problem rather than specifically a linear or nonlinear programming problem. It is amenable to a wide variety of large-scale problems that decouple or otherwise simplify when certain decision variables are fixed. This chapter presents the basic theory of LBBD and explains how classical Benders decomposition is a special case. It also describes branch and check, a variant of LBBD that solves the master problem only once. It illustrates in detail how Benders cuts and subproblem relaxations can be developed for some planning and scheduling problems. It then describes the role of LBBD in three large-scale case studies. The chapter concludes with an extensive survey of the LBBD literature, organized by problem domain, to allow the reader to explore how Benders cuts have been developed for a wide range of applications.},
  url={http://dx.doi.org/10.1007/978-3-030-22788-3_1}
}

@inbook{SchuttFSW15,
author="Schutt, Andreas
and Feydy, Thibaut
and Stuckey, Peter J.
and Wallace, Mark G.",
title="A Satisfiability Solving Approach",
