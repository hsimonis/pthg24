@article{Yunes2005,
  author={Tallys H. Yunes and  Arnaldo V. Moura and  Cid C. de Souza},
  title={Hybrid Column Generation Approaches for Urban Transit Crew Management Problems},
  doi={10.1287/trsc.1030.0078},
